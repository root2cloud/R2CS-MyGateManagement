<odoo>
    <data noupdate="1">
        <!-- Cron job to update vaccination status daily -->
        <record id="ir_cron_update_pet_vaccination_status" model="ir.cron">
            <field name="name">Update Pet Vaccination Status</field>
            <field name="model_id" ref="model_pet_management"/>
            <field name="state">code</field>
            <field name="code">model._cron_update_vaccination_status()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
